{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@vonage/cli",
  "version": "3.0.0-beta.1",
  "description": "Vonage CLI",
  "license": "Apache 2.0",
  "contributors": [
    {
      "name": "Paul Ardeleanu",
      "url": "https://github.com/pardel"
    },
    {
      "name": "Chuck \"MANCHUCK\" Reeves",
      "url": "https://github.com/manchuck"
    },
    {
      "name": "Chris Tankersley",
      "url": "https://github.com/dragonmantank"
    }
  ],
  "bin": {
    "vonage": "bin/vonage.js"
  },
  "files": [
    "src/**",
    "bin/**",
    "data/**"
  ],
  "scripts": {
    "eslint": "eslint",
    "jest": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "husky",
    "test": "jest --silent --verbose",
    "test:watch": "jest --verbose --silent --watchAll"
  },
  "lint-staged": {
    "package.json": [
      "npx sort-package-json"
    ],
    "*.js": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@vonage/auth": "1.12.0",
    "@vonage/jwt": "1.11.0",
    "@vonage/server-client": "1.16.0",
    "@vonage/server-sdk": "3.19.0",
    "ajv": "8.17.1",
    "camelcase": "6.3.0",
    "chalk": "4",
    "easy-table": "1.2.0",
    "json-diff": "1.0.6",
    "jsonwebtoken": "9.0.2",
    "snakecase": "1.0.0",
    "table": "6.8.2",
    "winston": "3.15.0",
    "yaml": "2.6.0",
    "yargs": "17.7.2",
    "yargs-parser": "21.1.1"
  },
  "devDependencies": {
    "@eslint/js": "9.12.0",
    "@faker-js/faker": "9.0.3",
    "@stylistic/eslint-plugin-js": "2.9.0",
    "eslint-plugin-jest": "28.8.3",
    "eslint-plugin-n": "17.11.1",
    "globals": "15.11.0",
    "husky": "9.1.6",
    "jest": "29.7.0",
    "lint-staged": "15.2.10",
    "winston-transport": "4.8.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
