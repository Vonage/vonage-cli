{
    "name": "@vonage/cli-plugin-applications",
    "description": "A plugin for Vonage CLI",
    "version": "0.0.0-alpha.0",
    "author": "Vonage Dev Rel <devrel@vonage.com>",
    "bugs": "https://github.com/Vonage/vonage-cli/issues",
    "dependencies": {
        "@oclif/command": "^1.8.0",
        "@oclif/config": "^1.17.0",
        "@oclif/parser": "^3.8.5",
        "@vonage/cli-utils": "1.0.0-alpha.0",
        "chalk": "^2.4.2",
        "lodash": "^4.17.20",
        "prompts": "^2.4.0",
        "tslib": "^2.1.0",
        "typescript": "^4.2.2",
        "unique-names-generator": "^4.3.1"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "files": [
        "/src",
        "/bin",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "homepage": "https://github.com/Vonage/vonage-cli",
    "keywords": [
        "oclif-plugin"
    ],
    "license": "Apache 2.0",
    "oclif": {
        "commands": "./dist/commands",
        "hooks": {
            "capabilities": [
                "./src/hooks/capabilities/capabilities"
            ]
        },
        "bin": "oclif-example",
        "devPlugins": [
            "@oclif/plugin-help"
        ]
    },
    "repository": "Vonage/vonage-cli",
    "scripts": {
        "postpack": "rm -f oclif.manifest.json",
        "posttest": "eslint . --ext .ts --config .eslintrc",
        "prepack": "rm -rf dist && tsc -b && oclif-dev manifest && oclif-dev readme",
        "test": "../../node_modules/.bin/jest test",
        "test-watch": "../../node_modules/.bin/jest test --watch",
        "version": "oclif-dev readme && git add README.md",
        "build": "rm -rf ./dist && tsc -b"
    },
    "devDependencies": {
        "@types/lodash.merge": "^4.6.6"
    }
}
