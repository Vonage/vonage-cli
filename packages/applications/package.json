{
    "name": "@vonage/cli-plugin-applications",
    "description": "A plugin for Vonage CLI",
    "version": "1.2.0",
    "author": "Vonage Dev Rel <devrel@vonage.com>",
    "bugs": "https://github.com/Vonage/vonage-cli/issues",
    "module": true,
    "dependencies": {
        "@oclif/command": "1.8.16",
        "@oclif/config": "1.18.3",
        "@oclif/core": "1.16.3",
        "@oclif/parser": "3.8.7",
        "@vonage/cli-utils": "^1.3.0",
        "chalk": "5.0.1",
        "cli-ux": "5.6.3",
        "filenamify": "5.1.1",
        "lodash": "4.17.21",
        "prompts": "2.4.2",
        "unique-names-generator": "4.7.1"
    },
    "engines": {
        "node": ">=12.0.0"
    },
    "files": [
        "/dist",
        "/bin",
        "/npm-shrinkwrap.json",
        "/node_modules",
        "/oclif.manifest.json"
    ],
    "homepage": "https://github.com/Vonage/vonage-cli",
    "keywords": [
        "oclif-plugin"
    ],
    "license": "Apache 2.0",
    "oclif": {
        "commands": "./dist/commands",
        "bin": "oclif-example",
        "devPlugins": [
            "@oclif/plugin-help"
        ]
    },
    "repository": "Vonage/vonage-cli",
    "scripts": {
        "postpack": "rm -f oclif.manifest.json",
        "prepack": "rm -rf dist && tsc -b && oclif-dev manifest && oclif-dev readme",
        "version": "oclif-dev readme && git add README.md",
        "build": "rm -rf ./dist && tsc -b"
    },
    "devDependencies": {
        "@oclif/test": "^2.1.1"
    }
}
