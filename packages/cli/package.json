{
  "name": "@vonage/cli",
  "version": "2.0.0",
  "description": "CLI to interact with Vonage APIs",
  "keywords": [
    "vonage"
  ],
  "homepage": "https://github.com/Vonage/vonage-cli",
  "bugs": "https://github.com/Vonage/vonage-cli/issues",
  "repository": "Vonage/vonage-cli",
  "license": "Apache 2.0",
  "author": "Vonage Dev Rel <devrel@vonage.com>",
  "main": "dist/index.js",
  "bin": {
    "vonage": "./bin/run.js"
  },
  "files": [
    "bin",
    "dist/**",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "npx shx rm -rf dist tsconfig.tsbuildinfo",
    "compile": "npx tsc --build --verbose",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@oclif/core": "^2.8.5",
    "@oclif/plugin-autocomplete": "^2.2.0",
    "@oclif/plugin-commands": "^2.2.15",
    "@oclif/plugin-help": "^5.2.9",
    "@oclif/plugin-not-found": "^2.3.23",
    "@oclif/plugin-which": "^2.2.20",
    "@vonage/cli-core": "^2.0.0",
    "@vonage/cli-jwt": "^2.0.0",
    "ts-node": "^10.9.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "oclif": {
    "bin": "vonage",
    "plugins": [
      "@oclif/plugin-not-found",
      "@oclif/plugin-help",
      "@oclif/plugin-commands",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-which",
      "@vonage/cli-core",
      "@vonage/cli-jwt"
    ]
  }
}
