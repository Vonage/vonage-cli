{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@vonage/cli-core",
  "version": "2.1.0",
  "description": "Core classes for vonage CLI's ",
  "keywords": [
    "vonage"
  ],
  "homepage": "https://github.com/Vonage/vonage-cli/tree/main/packages/core#readme",
  "bugs": {
    "url": "https://github.com/Vonage/vonage-cli/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vonage/vonage-cli.git"
  },
  "license": "Apache-2.0",
  "contributors": [
    {
      "name": "Chuck 'MANCHUCK' Reeves",
      "url": "https://github.com/manchuck"
    }
  ],
  "main": "lib/index.js",
  "type": "module",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "dist/**",
    "lib/**",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "npx shx rm -rf dist tsconfig.tsbuildinfo",
    "compile": "npx tsc --build --verbose",
    "prepublishOnly": "npm run build"
  },
  "oclif": {
    "commands": "./lib/commands",
    "topics": {
      "config": {
        "description": "Manage Vonage config"
      }
    }
  },
  "dependencies": {
    "@oclif/core": "4.0.11",
    "@vonage/cli-fs": "2.0.0",
    "@vonage/cli-ux": "2.0.0",
    "@vonage/jwt": "^1.10.0",
    "chalk": "^4",
    "debug": "^4.3.4",
    "lodash.kebabcase": "^4.1.1",
    "lodash.snakecase": "^4.1.1",
    "lodash.startcase": "^4.4.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@oclif/test": "4.0.7",
    "@types/debug": "4.1.12",
    "@types/lodash.kebabcase": "4.1.9",
    "@types/lodash.snakecase": "4.1.9",
    "@types/lodash.startcase": "4.4.9",
    "expect-type": "^0.19.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
