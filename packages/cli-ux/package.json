{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@vonage/cli-ux",
  "version": "2.0.0",
  "description": "CLI Plugin for common UX features",
  "homepage": "https://github.com/Vonage/vonage-cli/tree/main/packages/cli-ux#readme",
  "bugs": {
    "url": "https://github.com/Vonage/vonage-cli/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vonage/vonage-cli.git"
  },
  "license": "Apache-2.0",
  "contributors": [
    {
      "name": "Chuck \"MANCHUCK\" Reeves",
      "url": "https://github.com/manchuck"
    }
  ],
  "type": "module",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "npx shx rm -rf dist tsconfig.tsbuildinfo",
    "compile": "npx tsc --build --verbose",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@oclif/core": "4.0.11",
    "chalk": "^4",
    "terminal-link": "3.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
